package com.bridgelabz.functional;

public class PrintInitials
{
	public static void main(String args[])
	{

		int i,j;
		for(i=1;i<=9;i++)
		{
			for(j=1;j<=14;j++)
			{
				if(j==1 || j==2)
					
				{
					System.out.print("*");

				}
				else
				{
					System.out.print(" ");

				}

			}
			System.out.println();
		}
	}
}
