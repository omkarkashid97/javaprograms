package com.bridgelabz.functional;

public class PrintInitials
{
	public static void main(String args[])
	{

		int i,j;
		for(i=1;i<=10;i++)
		{
			for(j=1;j<=9;j++)
			{
				if(i==1 || i==2 || j==9 || j==2)
				{
					System.out.println("*");

				}
				else
				{
					System.out.print(" ");

				}
			}
		}
	}
}
