package com.bridgelabz.functional;

public class PrintInitials
{
	public static void main(String args[])
	{

		int i,j;
		for(i=1;i<=9;i++)
		{
			for(j=1;j<=14;j++)
			{
				if(i==1 && i==9 || j>=1 || j<=10 || ((i==2 || i==8) &&(j==11 || j==12)) || ((i==3 || i==7) &&(j==12 || j==13)) || ((i==4 || i==6 || i==5) &&(j==13 || j==14)))
					
				{
					System.out.print("*");

				}
				else
				{
					System.out.print(" ");

				}

			}
			System.out.println();
		}
	}
}
