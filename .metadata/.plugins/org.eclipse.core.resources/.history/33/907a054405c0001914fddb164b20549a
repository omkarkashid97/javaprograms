package com.bridgelabz.functional;

public class PrintInitials
{
	public static void main(String args[])
	{

		int i,j;
		for(i=1;i<=9;i++)
		{
			for(j=1;j<=14;j++)
			{
				if(j==1 || j==2 || (i==5 && j<6) || ((i==4 || i==6)&& j>4 && j<8) || ((i==3 || i==7)&& j>6 && j<10) || ((i==2 || i==8)&& j>8 && j<12) || ((i==1 || i==9)&& j>10 && j<14))
					
				{
					System.out.print("*");

				}
				else
				{
					System.out.print(" ");

				}

			}
			System.out.println();
		}
		
		System.out.println();
		System.out.println();

		for(i=1;i<=9;i++)
		{
			for(j=1;j<=14;j++)
			{
				if(j==1 || j==2 || (i==1 &&(j<11)) || ((i==2 || i==8) && (j>10 && j<13)) || ((i==3 || i==7) && (j>11 && j<14)) || ((i==4 || i==5 || i==6) && (j==13 || j==14)) || (i==9 && j<11))
					
				{
					System.out.print("*");

				}
				else
				{
					System.out.print(" ");

				}

			}
			System.out.println();
		}
		System.out.println();
		System.out.println();
		
		for(i=1;i<=9;i++)
		{
			for(j=1;j<=14;j++)
			{
				if(i==1 && (j==1||j==2||j==15||j==30||j==31))
					
				{
					System.out.print("*");

				}
				else
				{
					System.out.print(" ");

				}

			}
			System.out.println();
		}
		

	}
}
